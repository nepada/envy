{
    "name": "nepada/envy",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Helpers for extracting, validating and type-casting data from environment variables",
    "keywords": ["nette", "env", "configuration"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/envy/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4",
        "nette/utils": "^3.1.3@dev"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "nepada/coding-standard": "^7.3",
        "nepada/phpstan-nette-tester": "^0.3.1",
        "nette/tester": "^2.3.5",
        "pepakriz/phpstan-exception-rules": "^0.11.6",
        "phpstan/phpstan": "^0.12.78",
        "phpstan/phpstan-nette": "^0.12.14",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "spaze/phpstan-disallowed-calls": "^1.0.1",
        "nette/bootstrap": "^3.0",
        "nette/di": "^3.0.5@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\Envy\\": "src/Envy/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
